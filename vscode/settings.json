{
	"editor.lineNumbers": "on",
	"editor.rulers": [120],
	"editor.formatOnType": true,
	"editor.copyWithSyntaxHighlighting": false,
	"editor.emptySelectionClipboard": false,
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.snippetSuggestions": "top",
	"editor.minimap.enabled": true,
	"editor.minimap.showSlider": "always",
	"editor.hover.enabled": true,
	"editor.matchBrackets": "never",
	"editor.colorDecorators": true,
	"editor.lightbulb.enabled": "on",
	"editor.selectionHighlight": false,
	"editor.renderLineHighlight": "all",
	"editor.renderControlCharacters": false,
	"editor.occurrencesHighlight": "off",
	"editor.hideCursorInOverviewRuler": true,
	"editor.gotoLocation.multipleReferences": "goto",
	"editor.gotoLocation.multipleDefinitions": "goto",
	"editor.gotoLocation.multipleDeclarations": "goto",
	"editor.gotoLocation.multipleImplementations": "goto",
	"editor.gotoLocation.multipleTypeDefinitions": "goto",
	"editor.fontLigatures": true,
	"editor.lineHeight": 24,
	"editor.insertSpaces": true,
	"editor.formatOnSave": true,
	"editor.detectIndentation": false,
	"editor.guides.indentation": false,

	// "editor.fontFamily": "'ComicShannsMono Nerd Font', 'Droid Sans Mono', 'monospace', monospace",
	// "editor.fontFamily": "'FiraCode Nerd Font Mono', 'Droid Sans Mono', 'monospace', monospace",
	// "editor.fontFamily": "'FantasqueSansM Nerd Font Mono', 'Droid Sans Mono', 'monospace', monospace",
	// "editor.fontFamily": "'Hasklug Nerd Font Mono', 'Droid Sans Mono', 'monospace', monospace",
	"editor.fontFamily": "'BlexMono Nerd Font', 'Droid Sans Mono', 'monospace', monospace",

	// * Window
	"window.newWindowDimensions": "inherit",
	"window.zoomLevel": 2,
	"window.customTitleBarVisibility": "windowed",

	// * Terminal
	"terminal.integrated.fontFamily": "ComicShannsMono Nerd Font, Droid Sans Mono, monospace, monospace",
	// "terminal.integrated.fontFamily": "FiraCode Nerd Font, Droid Sans Mono, monospace, monospace",
	// "terminal.integrated.fontFamily": "'BlexMono Nerd Font', 'Droid Sans Mono', 'monospace', monospace",
	// "terminal.integrated.fontFamily": "'Hasklug Nerd Font Mono', 'Droid Sans Mono', 'monospace', monospace",

	"terminal.integrated.lineHeight": 1,
	"terminal.integrated.fontSize": 12,

	// * Workbench
	"workbench.sideBar.location": "left",
	"workbench.activityBar.location": "default",
	"workbench.activityBar.orientation": "horizontal",
	"workbench.editor.enablePreview": false,
	"workbench.statusBar.visible": true,
	"workbench.tree.enableStickyScroll": false,
	"workbench.tree.renderIndentGuides": "onHover",
	"workbench.editor.showTabs": "multiple",
	"workbench.tips.enabled": false,
	"workbench.editor.enablePreviewFromQuickOpen": false,
	"workbench.tree.indent": 18,
	"workbench.colorCustomizations": {
		// "editorCursor.foreground": "#40b808"
		// "editorCursor.foreground": "#be9b33"
		// "editorCursor.foreground": "#ba4d05"
		// "editorCursor.foreground": "#a4dfcc"
		// "editorCursor.foreground": "#326435"
		// "editorCursor.foreground": "#b87810"
		// "editorCursor.foreground": "#049090"
		// "editorCursor.foreground": "#868887"
	},

	// * Diff Editor
	"diffEditor.ignoreTrimWhitespace": false,
	"diffEditor.renderSideBySide": false,

	// * Git
	"git.decorations.enabled": true,
	"git.openRepositoryInParentFolders": "always",
	"git.autofetch": true,

	// * Files
	"files.trimTrailingWhitespace": true,
	"files.insertFinalNewline": true,
	"files.trimFinalNewlines": true,
	"files.autoSave": "afterDelay",
	"files.associations": {
		"*.html": "html"
	},

	// * Explorer
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"explorer.compactFolders": false,

	// * Error Lens
	"errorLens.gutterIconsEnabled": true,
	"errorLens.messageBackgroundMode": "none",

	// * Stuff
	"emmet.extensionsPath": [""],
	"chat.editor.fontSize": 16,
	"markdown.preview.fontSize": 16,
	"vsicons.dontShowNewVersionMessage": true,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"breadcrumbs.enabled": true,
	"scm.diffDecorations": "none",
	"liveServer.settings.donotShowInfoMsg": true,
	"problems.decorations.enabled": true,
	"makefile.configureOnOpen": true,

	// * Spell Checking
	// "cSpell.blockCheckingWhenTextChunkSizeGreaterThan": 20000,
	// "cSpell.blockCheckingWhenAverageChunkSizeGreaterThan": 20000,

	// * Indentation
	"indentRainbow.errorColor": "rgba(128,32,32,0.6)",
	"indentRainbow.tabmixColor": "rgba(128,32,96,0.6)",
	"indentRainbow.indicatorStyle": "light",

	// * Eslint
	// "eslint.format.enable": true,
	// "eslint.problems.shortenToSingleLine": true,

	// * APC
	// "apc.stylesheet": {
	// 	".anysphere-markdown-container-root": "font-size: 16px; line-height: 1.4;"
	// },

	// * Prettier
	"prettier.enable": true,
	"prettier.singleQuote": true,
	"prettier.tabWidth": 4,
	"prettier.useTabs": true,
	"prettier.trailingComma": "all",

	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 2
	},

	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 2
	},

	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 4
	},

	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 4
	},

	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 4
	},

	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 4
	},

	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 4
	},

	"[css]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.tabSize": 2
	},

	"[go]": {
		"editor.defaultFormatter": "golang.go",
		"editor.tabSize": 4
	},

	"extensions.experimental.affinity": {
		"asvetliakov.vscode-neovim": 1
	},

	"editor.tokenColorCustomizations": {
		"[Tokyo Night]": {
			"textMateRules": [
				{
					"scope": [
						"comment keyword.codetag.notation",
						"comment.block.documentation keyword",
						"comment.block.documentation storage.type.class"
					],
					"settings": {
						"foreground": "#bb9af7"
					}
				},
				{
					"scope": ["comment.block.documentation entity.name.type.instance"],
					"settings": {
						"foreground": "#73daca",
						"fontStyle": "italic"
					}
				},
				{
					"scope": ["comment.block.documentation entity.name.type punctuation.definition.bracket"],
					"settings": {
						"foreground": "#bb9af7"
					}
				},
				{
					"scope": ["comment.block.documentation variable"],
					"settings": {
						"foreground": "#e0af68",
						"fontStyle": "italic"
					}
				}
			]
		},
		"textMateRules": [
			{
				"name": "One Dark italic",
				"scope": [
					"comment",
					"entity.other.attribute-name",
					"keyword",
					"markup.underline.link",
					"storage.modifier",
					"storage.type",
					"string.url",
					"variable.language.super",
					"variable.language.this"
				],
				"settings": {
					"fontStyle": "italic"
				}
			},
			{
				"name": "One Dark italic reset",
				"scope": [
					"keyword.operator",
					"keyword.other.type",
					"storage.modifier.import",
					"storage.modifier.package",
					"storage.type.built-in",
					"storage.type.function.arrow",
					"storage.type.generic",
					"storage.type.java",
					"storage.type.primitive"
				],
				"settings": {
					"fontStyle": ""
				}
			}
		]
	},
	"workbench.iconTheme": "vscode-icons",
	"prettier.printWidth": 120,
	"nebulaOni.update.restoreConfiguration": {
		"themeSettings": {
			"background": "Grey",
			"main": "Bumblebee",
			"highlight": "Ultra Violet",
			"focusBorder": "ON",
			"fontStyle": "Italic",
			"syntaxBrightness": "Normal",
			"comments": "Dark Violet",
			"favorite": "OFF"
		},
		"favoriteSettings": {
			"background": "Glacial Blue",
			"main": "Sakura",
			"highlight": "Ultra Violet",
			"focusBorder": "ON",
			"fontStyle": "Italic",
			"syntaxBrightness": "Dimmed",
			"comments": "Dark Magenta",
			"favorite": "Nebula Oni (Pegasus)"
		}
	},
	"editor.fontSize": 15
}

