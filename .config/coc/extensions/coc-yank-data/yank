3eac662497d1c90eca58330734fd8a3b|file:///Users/basilalbader/dev/Scrimba/Javascript/BlackJack-game/script.js|180|1|V|javascript
	/* Create a function checkPlayerStatus() */
	const checkPlayerStatus = sum => {
		const gameMessage = document.querySelector('#message-el');
		let message = '';
	
		if (sum <= 20) {
			message = 'Hit?';
		} else if (sum === 21) {
			message = 'BlackJack!!';
			newPlayer.hasBlackJack = true;
			resetGame();
		} else {
			message = 'You Loose :(';
			newPlayer.isAlive = false;
			resetGame();
		}
	
		gameMessage.textContent = message;
	};
