3281ad4fe72114d1c5564cccbe5ed52b|file:///Users/basilalbader/dev/Scrimba/Javascript/BlackJack-game/cards.js|23|1|V|javascript
	// Get random number
	const getRandomNumber = (min, max) => {
		const rndNbr = Math.floor(((Math.random() * (max - min)) + min));
	
		if (rndNbr === 1) {
			return 11;
		}
	
		if (rndNbr > 10) {
			return 10;
		}
	
		return rndNbr;
	};
	
	
