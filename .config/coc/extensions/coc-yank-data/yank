	/* Get random number function */
	const getRandomNbr = (min, max) => {
		const random = Math.floor((Math.random() * (max - min)) + min);
	
		if (random === 1) {
			return 11;
		}
	
		if (random > 10) {
			return 10;
		}
	
		return random;
	};
223d2df066d9f7a977dc05444bf22a4f|file:///Users/basilalbader/dev/Scrimba/Javascript/BlackJack-game/script.js|148|1|V|javascript
	
	/* Get random number function */
	const getRandomNbr = (min, max) => {
		const random = Math.floor((Math.random() * (max - min)) + min);
	
		if (random === 1) {
			return 11;
		}
	
		if (random > 10) {
			return 10;
		}
	
		return random;
	};
